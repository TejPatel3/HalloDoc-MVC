                                </div>
                                <div class="fw-bolder fs-4 p-2 text-start">
                                    @Model.requests.Count(m => m.Status == 6)
                                </div>
                            </div>
                            <div class="bottomarrow"></div>
                        </button>
                    </div>
                    <div class="allstatus">
