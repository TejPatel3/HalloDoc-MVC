<script>

    const radiouttons = document.querySelectorAll('.radio-under');
    const radioDivs = document.querySelectorAll('.check-activeradio');

    console.log("1" + radiouttons)
    console.log("2" + radioDivs)
    radiouttons.forEach((radio, index) => {
        radio.addEventListener('click', () => {
            radioDivs.forEach((div, divIndex) => {
                if (index === divIndex) {
                    div.classList.add('selected-border');
                } else {
                    div.classList.remove('selected-border');
                }
            });
        });
    });

</script>