@* cancel case model *@
<div class="modal fade rounded-3 shadow" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content ">
            <div class="modal-header bg-info">
                <h5 class="modal-title text-white" id="exampleModalLabel">cancel Confirmation</h5>
                <button class="btn" type="button" data-dismiss="modal">
                    <i class="fa-solid fa-xmark" style="color: #ffffff;"></i>
                </button>
            </div>
            <form asp-action="cancelCaseModal" method="post">
                <div class="modal-body">
                    <div class="requestid"></div>
                    <input type="number" class="requestid" name="id" hidden />
                    <h6 class="my-3 text-secondary fw-normal">Patient Name: <span class="text-info fw-normal pname"></span> </h6>
                    <select name="casetagname" id="CancelModalFirstDropDownSelect" class="form-select form-select-lg  fs-6 shadow-none ModalFirstDropDown" aria-label=".form-select-lg example">
                        <option class="fs-6" value="" selected hidden>Reasons For Cancellation</option>
                        @foreach (var item in Model.caseTags)
                        {
                            <option class="fs-6" value="@item.Name">@item.Name</option>
                        }
                    </select>
                    <span class="text-danger" id="CancelModalFirstDropDownSpan"></span>
                    <div class="my-2 form-floating mt-3">

                        <textarea class="form-control shadow-none notes" asp-for="BlockNotes"
                                  placeholder="Provide Additional Note" id="CancelModalNote"
                                  style="height: 100px"></textarea>
                        <label for="floatingTextarea" class="text-secondary">Provide Additional Note</label>
                        <span class="text-danger" id="CancelModalNoteSpan"></span>

                    </div>
                </div>
                <div class="modal-footer border-0">
                    <button type="button" value="submit" class="btn btn-info text-white ModalSubminbtn" id="CancelModalSubminbtn"> Confirm</button>
                    <button type="button" class="btn btn-outline-info" data-dismiss="modal">cancel</button>
                </div>
            </form>
        </div>
    </div>
</div>


@* Assign Case Modal *@
<div class="modal fade rounded-3 shadow" id="AssignModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header bg-info">
                <h5 class="modal-title text-white" id="exampleModalLabel">Assign Request</h5>
                <button class="btn" type="button" data-dismiss="modal">
                    <i class="fa-solid fa-xmark" style="color: #ffffff;"></i>
                </button>
            </div>
            <form asp-action="AssignCase" method="post">
                <div class="modal-body">
                    <input type="number" class="requestid" name="id" hidden />

                    <label class="my-2 text-secondary  fw-normal mb-3" style="font-size:12.8px">To Assign This Request, search and selectanother Physician</label>
                    <div class="form-floating">
                        <select name="regionname" id="AssignModalFirstDropDownSelect" class="form-select form-select-lg  fs-6 SelectedRegionGivePhysicianList" aria-label=".form-select-lg example">
                            <option class="fs-6" value="" selected hidden">Narrow Search By Region</option>
                            @foreach (var item in Model.regions)
                            {
                                <option class="fs-6 regionselect" id="@item.RegionId">@item.Name</option>
                            }
                        </select>
                        <label for="floatingTextarea" class="text-secondary">Narrow Search By Region</label>
                        <span class="text-danger" id="AssignModalFirstDropDownSpan"></span>

                    </div>
                    <div class="form-floating ">
                        <select id="AssignModalSecondDropDownSelect" name="physicianid" asp-for="PhysicianId" class="form-select pt-2 mt-3 fs-6 form-control physiciandrop" aria-label=".form-select-lg example">
                            <option class="fs-6" value="" selected hidden>Select Physician</option>
                        </select>
                        <span class=" text-danger" id="AssignModalSecondDropDownSpan"></span>
                    </div>

                    <div class="my-2 form-floating mt-3">

                        <textarea class="form-control" id="AssignModalNote"
                                  placeholder="Description" asp-for="BlockNotes"
                                  style="height: 100px"></textarea>
                        <label for="floatingTextarea" class="text-secondary">Description</label>
                        <span class="text-danger" id="AssignModalNoteSpan"></span>

                    </div>
                </div>
                <div class="modal-footer border-0">
                    <button type="button" class="btn btn-info text-white" id="AssignModalSubminbtn">Confirm</button>
                    <button type="button" class="btn btn-outline-info" data-dismiss="modal">cancel</button>
                </div>
            </form>
        </div>
    </div>
</div>


